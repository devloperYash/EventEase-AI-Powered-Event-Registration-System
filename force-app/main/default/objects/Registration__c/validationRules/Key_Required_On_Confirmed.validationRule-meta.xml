<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
  <fullName>Key_Required_On_Confirmed</fullName>
  <active>true</active>
  <errorConditionFormula>AND(ISPICKVAL(Status__c, "Confirmed"), ISBLANK(Attendee_Event_Key__c))</errorConditionFormula>
  <errorMessage>Composite key is required when status is Confirmed.</errorMessage>
  <errorDisplayField>Attendee_Event_Key__c</errorDisplayField>
</ValidationRule>
